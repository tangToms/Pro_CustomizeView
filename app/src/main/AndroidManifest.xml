<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pro_customizeview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustomzizeActivity"></activity>
        <activity android:name=".activity.ScrollViewActivity"></activity>
        <activity android:name=".activity.TouchEventActivity"></activity>
        <activity android:name=".activity.ButtonActivity"></activity>
        <activity android:name=".activity.SlideMenuActivity"></activity>
        <activity android:name=".activity.ClockActivity"></activity>
    </application>
</manifest>
